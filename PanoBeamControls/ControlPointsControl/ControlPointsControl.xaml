<UserControl x:Class="PanoBeam.Controls.ControlPointsControl.ControlPointsControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:controls="clr-namespace:PanoBeam.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Canvas Name="Grid0">
        <Canvas Name="LineCanvas" Visibility="Collapsed" />
        <controls:DragCanvas x:Name="Canvas0" AllowDragging="False" AllowDragOutOfView="False"
                      Margin="0,0,0,0"
                 MouseMove="Canvas0_MouseMove">
        </controls:DragCanvas>
    </Canvas>
</UserControl>

